{
  "expo": {
    "name": "cleaning-app",
    "slug": "cleaning-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "cleaningapp",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.cleaningapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.anonymous.cleaningapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo-splash.png",
          "backgroundColor": "#000",
          "resizeMode": "cover"
        }
      ],
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "147f08d5-9137-4524-b65d-a018134b2a49"
      }
    },
    "owner": "asimrauf"
  }
}
